.col-xs-12
    nav.navbar.navbar-default.navbar-fixed-top
        .container-fluid
            .navbar-header
                a.navbar-brand#navbar-drawer
                    i.fa.fa-lg.fa-navicon
                    = ' '
                img(src='./images/logo.png')#navbar-logo
                img(src='./images/banner.png')#navbar-banner
            .collapse.navbar-collapse
                ul.nav.navbar-nav.navbar-right
                    li.nav-divider-vertical.hidden-xs
                    li.hidden-xs.nav-strengths.nav-strengths-tip
                        a
                            i.nav-icon.fa.fa-fw.fa-2x.fa-file-text-o
                    li.nav-divider-vertical.hidden-xs
                    li.dropdown
                        a.dropdown-toggle(data-toggle="dropdown", role="button", aria-expanded="false")
                            b#nav-username Username
                            i.fa.fa-fw.fa-lg.fa-caret-down
                        ul.dropdown-menu(role="menu")
                            li.nav-profile
                                a
                                    i.fa.fa-fw.fa-lg.fa-user
                                    = ' '
                                    | Account
                            li.divider
                            li.nav-about
                                a
                                    i.fa.fa-fw.fa-lg.fa-info
                                    = ' '
                                    | About Us
                            li.nav-report
                                a
                                    i.fa.fa-fw.fa-lg.fa-ambulance
                                    = ' '
                                    | Report A Bug
                            li.divider
                            li.nav-logout
                                a
                                    i.fa.fa-fw.fa-lg.fa-sign-out
                                    = ' '
                                    | Log Out

