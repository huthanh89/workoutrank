.row
    .col-xs-12
        #strength-modal-view
.row
    .col-xs-12
        ol.breadcrumb(style='margin-bottom: 5px;')
            li#strength-home
                a Home
            li.active#strength-journals
                span My Workouts
                i.fa.fa-fw.fa-lg.fa-book
            .pull-right
                i.fa.fa-fw.fa-lg.fa-question-circle#strength-help(style="cursor:pointer")
.row
    .col-sm-12
        .panel.panel-default
            .panel-heading.clearfix
                .pull-right
                    button.btn.btn-xs.btn-primary#strength-add
                        i.fa.fa-fw.fa.fa-plus
                        = ' '
                        span Add Workout
                .pull-right
                    #strength-filter-view
                h3.panel-title My Workouts
            .panel-body
                #strength-table-view
